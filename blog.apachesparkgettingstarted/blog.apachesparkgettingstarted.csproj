<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>netcoreapp3.0</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Spark" Version="0.4.0" />
    </ItemGroup>

    <ItemGroup>
      <None Update="data\lyrics.csv">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </None>
      <None Update="data\songdata.csv">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </None>
      <None Update="data\seattle-cultural-space-inventory.csv">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </None>
    </ItemGroup>

    <ItemGroup>
      <Content Include="bin\Debug\netcoreapp3.0\microsoft-spark-2.3.x-0.4.0.jar" />
      <Content Include="bin\Debug\netcoreapp3.0\microsoft-spark-2.4.x-0.4.0.jar" />
    </ItemGroup>

</Project>
